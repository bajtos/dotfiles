// Place your key bindings in this file to overwrite the defaults
[
  {
    "key": "shift+cmd+t", "command": "workbench.action.tasks.test" },
  { "key": "ctrl+]", "command": "editor.action.goToDeclaration", "when": "editorTextFocus" },
  { "key": "ctrl+t", "command": "workbench.action.navigateBack", "when": "editorTextFocus" },
  { "key": "ctrl+o", "command": "workbench.action.navigateBack",  "when": "editorTextFocus" },
  { "key": "ctrl+n", "command": "editor.action.triggerSuggest", "when": "editorHasCompletionItemProvider && editorTextFocus && !editorReadonly" }
]
